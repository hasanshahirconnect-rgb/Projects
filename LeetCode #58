int lengthOfLastWord(char *s) {
    int len = strlen(s);
    int count = 0;

    // Start from the end and skip spaces
    int i = len - 1;
    while (i >= 0 && s[i] == ' ')
        i--;

    // Count characters of the last word
    while (i >= 0 && s[i] != ' ') {
        count++;
        i--;
    }

    return count;
}

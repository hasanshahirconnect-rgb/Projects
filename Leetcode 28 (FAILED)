#include <string.h>

int strStr(char* haystack, char* needle) {
    char str[50];
    int first;
    for (int i=0; i<strlen(haystack); i++) {
        if (haystack[i] == needle[i]) {
            for (int j=0; j<strlen(needle);j++) {
                str[j] = strcpy(haystack[i])
            }
        }

    }
}

#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char** fizzBuzz(int n, int* returnSize) {
    *returnSize = n; 

    char** answer = (char**)malloc(n * sizeof(char*));

    for (int i = 0; i < n; i++) {
        answer[i] = (char*)malloc(10 * sizeof(char));

        if ((i + 1) % 3 == 0 && (i + 1) % 5 == 0) {
            strcpy(answer[i], "FizzBuzz");
        }
        else if ((i + 1) % 3 == 0) {
            strcpy(answer[i], "Fizz");
        }
        else if ((i + 1) % 5 == 0) {
            strcpy(answer[i], "Buzz");
        }
        else {
            sprintf(answer[i], "%d", i + 1);
        }
    }

    return answer;
}

#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main(void) {
    int result = 0, x = 0;
    char str1[100], str2[100];
    scanf("%s %s", str1, str2);
    for (int i = 0; str1[i] != '\0'; i++) {	
        str1[i] = tolower(str1[i]);
    }
    for (int i = 0; str2[i] != '\0'; i++) {
        str2[i] = tolower(str2[i]);
    }
	result = strcmp(str1, str2);
	if (result > 0) {
		printf("1");
	}
	else if (result < 0) {
		printf("-1");
	}
	else {
		printf("0");
	}
	return 0;
}
